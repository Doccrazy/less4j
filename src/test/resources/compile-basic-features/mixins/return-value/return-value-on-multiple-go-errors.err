.variableMixin1() {
  @first: 10;
  second-in-first: @second;
  third-in-first: @third;
}

.variableMixin2() {
  third-in-second: @third;
  @second: 20;
}

.variableMixin3(...) {
  @third: 30;
}

#call-variableMixin {
  first: @first;
  second: @second;
  third: @third;
  .variableMixin1();
  .variableMixin2();
  .variableMixin3();
}